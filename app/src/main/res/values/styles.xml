<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="guiline_vertical">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="guiline_horizontal">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="text_semibold">
        <item name="android:layout_width">wrap_content</item>
       <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">@font/montserrat_semibold</item>
    </style>
    <style name="text_medium">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">@font/montserrat_medium</item>
    </style>
    <style name="text_regular">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">@font/montserrat_regular</item>
    </style>
    <style name="HiddenTitleTheme">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
    </style>
</resources>